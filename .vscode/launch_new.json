  "version": "0.2.0",
  "configurations": [
    {
      "name": "chatServer Linux",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug/chatserver/chatServer",
      "args": ["localhost", "8080", "chatServer.log"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
